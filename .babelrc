{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 Chrome versions"
              ],
              "node": "current"
            },
            "useBuiltIns": true,
            "loose": true
          }
        ]
      ],
      "plugins": [
        "external-helpers",
        "syntax-jsx",
        "transform-react-jsx",
        "transform-class-properties",
        [
          "csjs-postcss",
          {
            "plugins": [
              [
                "autoprefixer",
                {
                  "browsers": [
                    "> 2%"
                  ]
                }
              ]
            ]
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "> 15% in BR"
              ],
              "node": "7"
            },
            "useBuiltIns": true,
            "loose": true
          }
        ]
      ],
      "plugins": [
        "external-helpers",
        "syntax-jsx",
        "transform-react-jsx",
        "transform-react-inline-elements",
        "transform-react-constant-elements",
        "transform-class-properties",
        [
          "csjs-postcss",
          {
            "plugins": [
              [
                "autoprefixer",
                {
                  "browsers": [
                    "> 15%"
                  ]
                }
              ]
            ]
          }
        ]
      ]
    }
  }
}